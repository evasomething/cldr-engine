{
  "name": "@phensley/cldr",
  "version": "0.14.8",
  "description": "Internationalization in Typescript with Unicode CLDR, batteries included",
  "main": "lib/index.js",
  "module": "lib-es/index.js",
  "types": "lib/index.d.ts",
  "author": "Patrick Hensley <pathensley@gmail.com>",
  "license": "Apache-2.0",
  "homepage": "https://github.com/phensley/cldr-engine/",
  "repository": {
    "type": "git",
    "url": "https://github.com/phensley/cldr-engine"
  },
  "keywords": [
    "cldr",
    "unicode",
    "i18n",
    "intl",
    "internationalization",
    "locale",
    "math",
    "arbitrary-precision",
    "pluralization",
    "decimal",
    "globalize"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "api": "api-extractor run -c ./etc/api-extractor.json",
    "api:local": "yarn api -l",
    "build": "yarn build:cj && yarn build:es && yarn build:packs && yarn build:rollup",
    "build:cj": "tsc -p ./tsconfig.json",
    "build:es": "tsc -p ./tsconfig.json --module esnext --outDir lib-es",
    "build:lint": "tsc -p ./tsconfig.lint.json",
    "build:packs": "cldr-compiler pack -o ./packs",
    "build:watch": "tsc -p ./tsconfig.json -w --preserveWatchOutput",
    "build:rollup": "NODE_ENV=production rollup -c && yarn build:rollup-dts && rimraf dist/src",
    "build:rollup-dts": "dts-bundle-generator -o ./dist/cldrengine.umd.d.ts src/index.ts",
    "clean": "rimraf ./lib ./lib-es ./packs ./dist",
    "clean:full": "yarn clean && rimraf ./node_modules",
    "coverage": "jest --coverage --runInBand --no-cache",
    "install": "node ./scripts/extract.js",
    "lint": "tslint -p ./tsconfig.lint.json",
    "prepublish": "yarn build",
    "stress": "ts-node ./__stress__",
    "test": "jest",
    "test:watch": "jest --watch --coverage --coverageReporters=html"
  },
  "files": [
    "dist/",
    "lib/",
    "lib-es/",
    "packs/*.gz",
    "packs/sha*.txt",
    "packs/resource.json",
    "scripts/"
  ],
  "dependencies": {
    "@phensley/cldr-core": "^0.14.8",
    "@phensley/cldr-utils": "^0.14.8",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "beta",
    "@phensley/cldr-compiler": "^0.14.8",
    "@types/jest": "^23.3.14",
    "@types/node": "^10.12.21",
    "beautify-benchmark": "^0.2.4",
    "benchmark": "^2.1.4",
    "chalk": "^2.3.2",
    "dts-bundle-generator": "^2.1.0",
    "jest": "^23.6.0",
    "rimraf": "^2.6.2",
    "rollup": "^1.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-dts": "^0.13.0",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-typescript2": "^0.21.1",
    "rollup-plugin-uglify": "^6.0.2",
    "ts-jest": "^23.10.5",
    "ts-node": "6.1.1",
    "tslint": "5.14.0",
    "tslint-no-circular-imports": "^0.6.1",
    "typescript": "3.x",
    "uglify-es": "^3.3.9"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  }
}
