{
  "name": "@phensley/cldr-compiler",
  "version": "0.17.5",
  "description": "Compiler for @phensley/cldr",
  "main": "lib/index.js",
  "module": "lib-es/index.js",
  "types": "lib/index.d.ts",
  "author": "Patrick Hensley <pathensley@gmail.com>",
  "license": "Apache-2.0",
  "homepage": "https://github.com/phensley/cldr-engine/",
  "repository": {
    "type": "git",
    "url": "https://github.com/phensley/cldr-engine"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "cldr-compiler": "./bin/compiler.js"
  },
  "cldrversion": "35.1.0",
  "scripts": {
    "build": "yarn build:cj && yarn build:es && yarn download",
    "build:cj": "tsc -p ./tsconfig.json",
    "build:es": "tsc -p ./tsconfig.json --module esnext --outDir lib-es",
    "build:lint": "tsc -p ./tsconfig.lint.json",
    "build:watch": "tsc -p ./tsconfig.json -w --preserveWatchOutput",
    "clean": "rimraf ./lib ./lib-es",
    "clean:full": "yarn clean && rimraf ./node_modules",
    "coverage": "jest --coverage --runInBand --no-cache",
    "download": "node ./scripts/downloader.js",
    "lint": "tslint -p ./tsconfig.lint.json",
    "prepublish": "yarn build",
    "test:watch": "jest --watch --coverage --coverageReporters=html"
  },
  "files": [
    "bin",
    "lib/",
    "lib-es/"
  ],
  "dependencies": {
    "@phensley/cldr-core": "0.17.5",
    "@phensley/cldr-schema": "0.17.5",
    "@phensley/cldr-utils": "0.17.5",
    "@phensley/timezone": "0.17.5",
    "@types/prettier": "^1.10.0",
    "@types/request": "^2.48.1",
    "@types/yargs": "11.0.0",
    "chalk": "^2.4.2",
    "partial.lenses": "^14.17.0",
    "prettier": "^1.11.1",
    "request": "^2.88.0",
    "tar": "^4.4.8",
    "tslib": "^1.9.3",
    "yargs": "11.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^10.14.10",
    "beautify-benchmark": "^0.2.4",
    "benchmark": "^2.1.4",
    "jest": "^24.8.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-no-circular-imports": "^0.7.0",
    "typescript": "3.5.x"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  }
}
