{
  "name": "@phensley/cldr-compiler",
  "version": "0.6.0",
  "description": "Compiler for @phensley/cldr",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Patrick Hensley <pathensley@gmail.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phensley/cldr-engine"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "cldr-compiler": "./bin/compiler.js"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json",
    "build:watch": "tsc -p ./tsconfig.build.json -w --preserveWatchOutput",
    "clean": "rimraf ./lib",
    "coverage": "jest --coverage --runInBand --no-cache",
    "lint": "tslint -p ./tsconfig.json",
    "prepublish": "yarn build",
    "test:watch": "jest --watch --coverage --coverageReporters=html"
  },
  "files": [
    "bin",
    "lib/"
  ],
  "dependencies": {
    "@phensley/cldr-core": "^0.6.0",
    "@phensley/cldr-schema": "^0.6.0",
    "@types/node": "10.3.4",
    "@types/prettier": "^1.10.0",
    "@types/yargs": "^11.0.0",
    "cldr-data": "32.0.1",
    "partial.lenses": "^13.7.4",
    "prettier": "^1.11.1",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.1.1",
    "beautify-benchmark": "^0.2.4",
    "benchmark": "^2.1.4",
    "chalk": "^2.3.1",
    "jest": "^23.1.0",
    "rimraf": "^2.6.2",
    "tslint": "5.10.0",
    "tslint-no-circular-imports": "^0.2.1",
    "typescript": "~2.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>../../node_modules/ts-jest/preprocessor.js"
    }
  }
}
